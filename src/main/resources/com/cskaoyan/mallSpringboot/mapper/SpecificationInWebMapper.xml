<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cskaoyan.mallSpringboot.mapper.SpecificationInWebMapper">
    <insert id="insert">
        insert into cskaoyan_mall_goods_specification (goods_id,specification,`value`,pic_url) values
        (#{specification.goodsId},#{specification.specification},#{specification.value},
        #{specification.picUrl})
    </insert>

</mapper>